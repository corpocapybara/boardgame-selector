{
  "manifest_version": 2,

  "name": "Boardgame Selector",
  "description": "Lists boardgames from boardgamesgeek collection for given number of players",
  "version": "0.1",
  "homepage_url": "https://github.com/corpocapybara/boardgame-selector/",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.boardgamegeek.com/xmlapi2/"
  ]
}
